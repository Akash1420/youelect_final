<% include partials/header %>

<h1>Sign Up!</h1>
<form action="/register" method="POST">
  <div class="form-group">
    <label for="Your Name">Full Name</label>
    <input type="text" class="form-control" name="username" placeholder="Enter Name">
  </div>
  <div class="form-group">
    <label for="AadharNumber">Aadhar Number</label>
    <input type="text" class="form-control" name="aadhar" placeholder="Enter Your Aadhar Number">
  </div>
  <div class="form-group">
    <label for="Mobile">Phone Number</label>
    <input type="text" class="form-control" name="phone" placeholder="Enter Your Phone Number">
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" name="fname" placeholder="Enter email">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" name="password" class="form-control" placeholder="Password">
  </div>
  <center><button id="submit-button" type="submit" class="btn btn-success">Submit</button></center>
</form>

<% include partials/footer %>